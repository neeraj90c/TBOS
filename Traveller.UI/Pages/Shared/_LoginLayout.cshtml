<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Technotech</title>
    <link rel="stylesheet" href="~/Traveller.UI.styles.css" asp-append-version="true" />

    <!-- Bootstrap -->
    <link href="Layout2/css/Bootstrap.min.css" rel="stylesheet">
    <!-- Custom styles -->
    <link href="Layout2/css/style.css" rel="stylesheet">
    <link href="Layout2/css/responsive.css" rel="stylesheet">
    <!-- Web and icon Fonts -->
    <link href="Layout2/css/iconfonts.css" rel="stylesheet">

    <link rel="icon" href="~/clientdata/appimages/favicon.png" sizes="32x32">
    <link rel="icon" href="~/clientdata/appimages/favicon.png" sizes="192x192">
</head>
<body>
    <div>
            @RenderBody()
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js" asp-append-version="true"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <!-- scripts -->
    <script src="~/js/app.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>