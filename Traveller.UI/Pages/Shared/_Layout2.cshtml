<body id="page-top" class="sidebar-toggled">

    <!-- Page Wrapper -->
    <div id="wrapper">

        <div class="fade_bg d-block d-sm-block d-md-none"></div>
        <!-- Sidebar -->
        <div class="sidebar toggled" id="accordionSidebar">
            <ul class="navbar-nav accordion" id="MainNavigationBar">
            </ul>
        </div>
        <!-- End of Sidebar -->
        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
                <nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top">

                    <!-- Sidebar Toggler (Sidebar) -->
                    <div class="text-center d-none d-md-inline sidebarToggler">
                        <div id="sidebarToggle">
                            <span class="lines"></span>
                        </div>
                    </div>

                    <a class="sidebar-brand mr-3">
                        <div class="sidebar-brand-text"><img src="~/clientdata/appimages/headerlogo.png" class="img-responsive"></div>
                    </a>

                    <!-- Sidebar Toggle (Topbar) -->
                    <button id="sidebarToggleTop" class="btn btn-link d-md-none rounded-circle mr-3"> <i class="fa fa-bars"></i> </button>

                    <!-- Topbar Search -->
                    <form class="d-none d-sm-inline-block form-inline m-auto my-2 my-md-0 mw-100 navbar-search main_search">
                        @* <input type="text" id="globalSearch" class="form-control bg-light border-0 small"
                               placeholder="Search here..." onkeyup="GlobalSearch.SearchText()" aria-label="Search" aria-describedby="basic-addon2" autocomplete="off">
                        <img src="~/layout2/images/search_icon.svg">
                        <div class="search_div shadow" id="GlobalSearchList" style="display:none;">
                            <div class="search_list shadow">
                                <ul id="suggestionlist">
                                </ul>
                            </div>
                            <div id="GlobalSearchShowAll" style="height:35px; text-align:center;background-color: white; padding:10px 0px 10px 0px;border-radius:0px 10px 0px 10px;" onclick="GlobalSearch.ShowAll()">
                                <a href="#"><u></u></a>
                            </div>
                        </div> *@
                    </form>
                    <!-- Topbar Navbar -->
                    <ul class="navbar-nav top_navbar">
                        <li class="nav-item">
                            <a href="#" id="sidebarToggleTop" class="btn btn-link d-md-none nav-link">
                                <div class="hamburger">
                                    <span class="lines"></span>
                                </div>
                            </a>
                        </li>

                        <li class="nav-item">
                            <a href="#" class="nav-link d-md-none mob_logo">
                                <img src="~/clientdata/appimages/headerlogo.png" class="img-responsive">
                            </a>
                        </li>

                        <!-- Nav Item - Search Dropdown (Visible Only mobile screen) -->
                        <li class="nav-item dropdown no-arrow d-md-none ml-auto">
                            <a class="nav-link dropdown-toggle" href="#" id="searchDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <img src="~/layout2/images/search_icon.svg" style="width: 16px;">
                            </a>
                            <!-- Dropdown - Messages -->
                            <div class="dropdown-menu dropdown-menu-right p-2 shadow animated--grow-in shadow-lg" aria-labelledby="searchDropdown">
                                <form class="form-inline mr-auto w-100 navbar-search">
                                    <div class="input-group">
                                        <input type="text" class="form-control bg-light border-0 small" placeholder="Search here..." aria-label="Search" aria-describedby="basic-addon2">
                                        <div class="input-group-append">
                                            <button class="btn btn-primary" type="button"> <i class="fas fa-search fa-sm"></i> </button>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </li>

                        <li class="nav-item">
                            <div class="nav-link">
                                <a href="#" class="alert_icon">
                                    <div class="reddot"></div>
                                    <img src="~/layout2/images/notification_icon.svg">
                                </a>
                            </div>
                        </li>

                        <li class="nav-item">
                            <div class="nav-link">
                                <a href="#" class="alert_icon">
                                    <div class="reddot"></div>
                                    <img src="~/layout2/images/msg_icon.svg">
                                </a>
                            </div>
                        </li>

                        <li class="nav-item dropdown no-arrow">
                            <a class="nav-link dropdown-toggle" href="#" id="userDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <img id="HeaderUserImg" class="rounded-circle me-lg-2" src="~/img/users/user_1.jpg" alt="" style="width: 40px; height: 40px;">
                                <img id="MenuUserImg" class="rounded-circle" src="~/img/users/user_1.jpg" alt="" style="width: 40px; height: 40px; display:none;">
                            </a>
                            <div class="dropdown-menu dropdown-menu-right shadow-lg animated--grow-in userDropdown" aria-labelledby="userDropdown">
                                <div class="text-center py-2">
                                    <div class="font-sb h6 text_col2 mb-1" id="HeaderUserNameLabel">John Doe</div>
                                    <div class="mb-0" id="MenuUserNameLabel" style="display:none;">Jhon Doe</div>
                                    <div class="font-l text_col4 font-small" id="MenuUserTypeLabel">Admin</div>
                                    <div style="display:none;" id="MenuUserRoleIdLabel">Admin</div>
                                    @*<div class="font-l text_col4 font-small">ravi.amin@systel.com</div>*@
                                    <div class="dropdown-divider"></div>
                                    <div class="profile_setting">
                                        <a class="d-inline mr-1" href="#"> <i class="fas fa-user fa-sm fa-fw mr-1 text_col5"></i> Profile </a> |
                                        <a class="d-inline ml-1 mr-1" href="#"> <i class="fas fa-cogs fa-sm fa-fw mr-1 text_col5"></i> Settings </a> |
                                        <a class="d-inline ml-1" href="#"> <i class="fas fa-user-cog fa-sm fa-fw mr-1 text_col5"></i> Admin </a>
                                    </div>
                                    <div class="dropdown-divider"></div>
                                    <a href="#" class="btn btn-outline-secondary rounded-pill px-3" onclick="User.Signout()"> Logout </a>
                                    <div class="dropdown-divider"></div>
                                    <a href="#" class="dropdown-item" id="AppVersionA">@SessionObj.AppVersion</a>
                                    <a href="#" class="dropdown-item" id="LayoutNumber" style="display:none;">L2</a>
                                </div>
                            </div>
                        </li>
                    </ul>
                </nav>

                <!-- End of Topbar -->
                <div class="container-fluid" id="LayoutContainerFluid">
                    @RenderBody()
                    @*</main>*@
                </div>
            </div>
            <!-- End of Main Content -->
            <!-- Footer -->
            @* <footer class="sticky-footer bg-white">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto"> <span>Copyright © 2024 VNC. All rights reserved.</span> </div>
                </div>
            </footer> *@
            <!-- End of Footer -->

        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded-circle" href="#page-top"> <i class="fas fa-angle-up"></i> </a>


